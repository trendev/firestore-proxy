{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore",
            "allow-trailing-underscore"
        ],
        "trailing-comma": [
            true,
            {
                "singleline": "never",
                "multiline": {
                    "objects": "ignore",
                    "arrays": "always",
                    "functions": "never",
                    "typeLiterals": "ignore"
                }
            }
        ]
    },
    "rulesDirectory": []
}